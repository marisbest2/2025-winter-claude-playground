{
  "mcpServers": {
    "context7": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"],
      "env": {}
    },
    "mastra-docs": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@mastra/mcp-docs-server"],
      "env": {}
    },
    "government-records": {
      "type": "stdio",
      "command": "pnpm",
      "args": ["--filter", "@teaneck/agents", "mcp:serve"],
      "env": {}
    }
  }
}
