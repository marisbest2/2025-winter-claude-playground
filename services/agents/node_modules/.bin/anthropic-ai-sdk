#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/MROCHLIN/gc-git/2025-winter-claude-playground/node_modules/.pnpm/@anthropic-ai+sdk@0.71.2_zod@4.2.1/node_modules/@anthropic-ai/sdk/bin/node_modules:/Users/MROCHLIN/gc-git/2025-winter-claude-playground/node_modules/.pnpm/@anthropic-ai+sdk@0.71.2_zod@4.2.1/node_modules/@anthropic-ai/sdk/node_modules:/Users/MROCHLIN/gc-git/2025-winter-claude-playground/node_modules/.pnpm/@anthropic-ai+sdk@0.71.2_zod@4.2.1/node_modules/@anthropic-ai/node_modules:/Users/MROCHLIN/gc-git/2025-winter-claude-playground/node_modules/.pnpm/@anthropic-ai+sdk@0.71.2_zod@4.2.1/node_modules:/Users/MROCHLIN/gc-git/2025-winter-claude-playground/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/MROCHLIN/gc-git/2025-winter-claude-playground/node_modules/.pnpm/@anthropic-ai+sdk@0.71.2_zod@4.2.1/node_modules/@anthropic-ai/sdk/bin/node_modules:/Users/MROCHLIN/gc-git/2025-winter-claude-playground/node_modules/.pnpm/@anthropic-ai+sdk@0.71.2_zod@4.2.1/node_modules/@anthropic-ai/sdk/node_modules:/Users/MROCHLIN/gc-git/2025-winter-claude-playground/node_modules/.pnpm/@anthropic-ai+sdk@0.71.2_zod@4.2.1/node_modules/@anthropic-ai/node_modules:/Users/MROCHLIN/gc-git/2025-winter-claude-playground/node_modules/.pnpm/@anthropic-ai+sdk@0.71.2_zod@4.2.1/node_modules:/Users/MROCHLIN/gc-git/2025-winter-claude-playground/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@anthropic-ai/sdk/bin/cli" "$@"
else
  exec node  "$basedir/../@anthropic-ai/sdk/bin/cli" "$@"
fi
